                                        type="image/jpg">
                                <source srcset="{{ gallery.imageOne.ng_small_src }}" type="image/webp" media="(min-width: 400px)">
                                <source srcset="{{ gallery.imageOne.small_src }}" type="image/jpg" media="(min-width: 400px)">
